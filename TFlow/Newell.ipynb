   ],
   "source": [
    "k=0.001\n",
    "Q=0\n",
    "k_m=0\n",
    "while k<=150:\n",
    "    q=Newell(k)\n",
    "    if q>Q:\n",
    "        Q=q\n",
    "        k_m=k\n",
    "    k+=0.001\n",
    "print(k_m,Q)"
   ]
